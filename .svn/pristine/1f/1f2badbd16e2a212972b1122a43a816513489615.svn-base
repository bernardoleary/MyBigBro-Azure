using System;
using System.Collections.Generic;
using System.ComponentModel.DataAnnotations;
using System.Linq;
using System.Text;

namespace Infostructure.MyBigBro.Domain
{
    public class User : IUser
    {
        [Key]
        public int Id
        {
            get;
            set;
        }

        public string UserName
        {
            get;
            set;
        }

        public string Email
        {
            get;
            set;
        }

        public string Password
        {
            get; 
            set;
        }

        public bool IsAdmin
        {
            get; 
            set;
        }
    }
}
